definitions_to_help(char *args)
{

    if (args[1]) //just in case of syntax error
    {
        if (args[0] == '<')
            args[1] == 'IN_FILE';
        else if (args[0] == '>')
            args[1] == 'OUT_FILE';
        else if (args[0] == '>>')
            args[1] == 'APPEND_FILE';
        else if (args[0] == '<<')
            args[1] == 'LIMETER';
    }
}
